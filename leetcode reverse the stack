// User function Template for Java

class Solution {
    static void reverse(Stack<Integer> s) {
      if(s.isEmpty()){
          return ;
      }
      int t= s.pop();
      (s);
      isreverse(s,t);
    }
    static void isreverse(Stack<Integer>s,int item){
        if(s.isEmpty()){
            s.push(item);
            return;
        }
        int temp=s.pop();
        isreverse(s,item);
        s.push(temp);
    }
}
