class Solution {
    public boolean isPalindrome(String s) {
        s=s.toLowerCase();
        s=s.replaceAll("[^a-z0-9]", ""); 
        StringBuilder sc=new StringBuilder();
        for(int i =s.length()-1;i>=0;i--){
           sc.append(s.charAt(i));
        }
    
      return s.equals(sc.toString());
    }
}
